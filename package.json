{
  "name": "ember-component-showcase",
  "version": "1.0.0",
  "description": "The default blueprint for ember-cli addons.",
  "keywords": [
    "ember-addon"
  ],
  "repository": "wombatsecurity/ember-component-showcase",
  "license": "MIT",
  "author": "Nicholas McClay <nmcclay@wombatsecurity.com>",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint:js": "eslint .",
    "start": "ember serve -p 4800",
    "test": "npm-run-all lint:* test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*",
    "lint:hbs": "ember-template-lint ."
  },
  "dependencies": {
    "@glimmer/tracking": "^1.0.3",
    "broccoli-file-creator": "^2.1.1",
    "broccoli-filter": "^1.3.0",
    "broccoli-funnel": "^3.0.3",
    "broccoli-merge-trees": "^4.2.0",
    "documentation": "github:mastastealth/documentation#bump/deps",
    "ember-auto-import": "^1.10.0",
    "ember-cli-babel": "^7.23.0",
    "ember-cli-htmlbars": "^5.3.0",
    "ember-elsewhere": "1.2.0",
    "ember-prism": "^0.7.0",
    "js-beautify": "^1.11.0",
    "lunr": "^2.3.3",
    "node-uuid": "^1.4.8",
    "prismjs": "^1.23.0",
    "remarkable": "^2.0.1"
  },
  "devDependencies": {
    "@ember/optional-features": "^1.3.0",
    "@ember/render-modifiers": "^1.0.2",
    "@glimmer/component": "^1.0.1",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "broccoli-stew": "^3.0.0",
    "ember-classic-decorator": "^1.0.8",
    "ember-cli": "~3.19.0",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-sass": "10.0.1",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-uglify": "^3.0.0",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.1",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-power-select": "^4.0.3",
    "ember-qunit": "^4.6.0",
    "ember-resolver": "^8.0.0",
    "ember-source": "~3.19.0",
    "ember-source-channel-url": "^2.0.1",
    "ember-template-lint": "^2.9.0",
    "ember-try": "^1.4.0",
    "eslint": "^7.4.0",
    "eslint-plugin-ember": "^8.9.1",
    "eslint-plugin-node": "^11.1.0",
    "font-awesome": "~4.7.0",
    "git-repo-version": "1.0.2",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "qunit-dom": "^1.2.0",
    "sass": "^1.26.10"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
